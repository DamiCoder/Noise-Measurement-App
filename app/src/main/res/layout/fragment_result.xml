<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent">
    <TextView
            android:text="@string/probedetails_standard"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView50"
            android:textStyle="bold" app:layout_constraintTop_toBottomOf="@+id/result_placeview"
            app:layout_constraintStart_toStartOf="@+id/textView52"
            app:layout_constraintEnd_toEndOf="@+id/textView52" app:layout_constraintHorizontal_bias="0.105"
            android:layout_marginTop="16dp" android:fontFamily="@font/open_sans"/>
    <TextView
            android:text="@string/probedetails_comment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView51"
            app:layout_constraintStart_toStartOf="@+id/textView50"
            app:layout_constraintEnd_toEndOf="@+id/textView50" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintTop_toBottomOf="@+id/result_regulationview" android:layout_marginTop="12dp"
            android:textStyle="bold" android:fontFamily="@font/open_sans"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/result_resultview"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="64dp"
            android:textSize="48sp" android:fontFamily="@font/open_sans_bold"
            android:textColor="@color/successColorDark" app:layout_constraintHorizontal_bias="0.501"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/result_resultlabelview"
            android:textSize="28sp"
            android:fontFamily="@font/open_sans_semibold" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toBottomOf="@+id/result_resultview"
            android:layout_marginTop="16dp" android:textColor="@color/successColorDark"/>
    <TextView
            android:layout_width="301dp"
            android:layout_height="wrap_content" android:id="@+id/result_resultdescview"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="12dp" app:layout_constraintTop_toBottomOf="@+id/result_resultlabelview"
            android:gravity="clip_horizontal|center_horizontal" android:textColor="@color/darkGrey"
            android:fontFamily="@font/open_sans" app:layout_constraintHorizontal_bias="0.508"/>
    <TextView
            android:text="@string/probedetails_place"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView52"
            app:layout_constraintStart_toStartOf="parent"
            android:textStyle="bold" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.197" android:fontFamily="@font/open_sans"
            app:layout_constraintTop_toBottomOf="@+id/result_resultdescview" android:layout_marginTop="48dp"/>
    <TextView
            android:text="@string/probedetails_location"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView53"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/textView51" app:layout_constraintEnd_toEndOf="@+id/textView51"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintTop_toBottomOf="@+id/result_commentview" android:layout_marginTop="12dp"
            android:fontFamily="@font/open_sans"/>
    <TextView
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:id="@+id/result_locationview"
            app:layout_constraintTop_toTopOf="@+id/textView53"
            app:layout_constraintBottom_toBottomOf="@+id/textView53"
            android:autoSizeTextType="uniform"
            android:autoSizeMinTextSize="12sp"
            android:autoSizeMaxTextSize="100sp"
            android:autoSizeStepGranularity="2sp"
            app:layout_constraintStart_toEndOf="@+id/textView53" android:layout_marginStart="64dp"
            app:layout_constraintVertical_bias="0.0"/>
    <TextView
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:id="@+id/result_placeview"
            app:layout_constraintStart_toEndOf="@+id/textView52" android:layout_marginStart="60dp"
            app:layout_constraintTop_toTopOf="@+id/textView52"
            android:autoSizeTextType="uniform"
            android:autoSizeMinTextSize="12sp"
            android:autoSizeMaxTextSize="100sp"
            android:autoSizeStepGranularity="2sp"
            app:layout_constraintBottom_toBottomOf="@+id/textView52" app:layout_constraintVertical_bias="0.0"/>
    <TextView
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:id="@+id/result_commentview"
            app:layout_constraintStart_toEndOf="@+id/textView51" android:layout_marginStart="44dp"
            app:layout_constraintTop_toTopOf="@+id/textView51"
            android:autoSizeTextType="uniform"
            android:autoSizeMinTextSize="12sp"
            android:autoSizeMaxTextSize="100sp"
            android:autoSizeStepGranularity="2sp"
            app:layout_constraintBottom_toBottomOf="@+id/textView51" app:layout_constraintVertical_bias="0.0"/>
    <TextView
            android:text="@string/probedetails_rating"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView54"
            app:layout_constraintTop_toBottomOf="@+id/result_locationview"
            android:layout_marginTop="12dp" app:layout_constraintStart_toStartOf="@+id/textView53"
            app:layout_constraintEnd_toEndOf="@+id/textView53" app:layout_constraintHorizontal_bias="0.0"
            android:fontFamily="@font/open_sans" android:textStyle="bold"/>
    <TextView
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:id="@+id/result_ratingview"
            app:layout_constraintStart_toEndOf="@+id/textView54"
            android:layout_marginStart="32dp"
            app:layout_constraintTop_toTopOf="@+id/textView54"
            app:layout_constraintBottom_toBottomOf="@+id/textView54"
            app:layout_constraintVertical_bias="1.0"/>
    <TextView
            android:layout_width="180dp"
            android:layout_height="wrap_content" android:id="@+id/result_regulationview"
            app:layout_constraintStart_toEndOf="@+id/textView50" android:layout_marginStart="44dp"
            app:layout_constraintTop_toTopOf="@+id/textView50"
            app:layout_constraintBottom_toBottomOf="@+id/textView50"/>
    <Button
            android:id="@+id/result_addcommentbutton"
            android:layout_width="17dp"
            android:layout_height="17dp"
            app:layout_constraintTop_toTopOf="@+id/textView51"
            app:layout_constraintBottom_toBottomOf="@+id/textView51"
            style="?android:attr/borderlessButtonStyle"
            app:layout_constraintEnd_toStartOf="@+id/textView51"
            android:layout_marginEnd="5dp"
            android:background="@drawable/record_button_bg"
            android:padding="2dp" android:layout_marginTop="3dp"/>
    <Button
            android:id="@+id/result_addratingbutton"
            android:layout_width="17dp"
            android:layout_height="17dp"
            app:layout_constraintTop_toTopOf="@+id/textView54"
            app:layout_constraintBottom_toBottomOf="@+id/textView54"
            style="?android:attr/borderlessButtonStyle"
            app:layout_constraintEnd_toStartOf="@+id/textView54"
            android:layout_marginEnd="5dp"
            android:background="@drawable/record_button_bg"
            android:padding="2dp" android:layout_marginTop="3dp"/>
    <Button
            android:text="@string/continuebutton_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/result_continuebutton"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="16dp" android:layout_marginBottom="16dp"
            android:fontFamily="@font/open_sans_bold" android:background="@drawable/login_button_bg"/>
</androidx.constraintlayout.widget.ConstraintLayout>