<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent">

    <TextView
            android:text="@string/probedetails_standard"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView29"
            android:textStyle="bold" app:layout_constraintTop_toBottomOf="@+id/probedetails_placeview"
            app:layout_constraintStart_toStartOf="@+id/textView21"
            app:layout_constraintEnd_toEndOf="@+id/textView21" app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginTop="12dp" android:fontFamily="@font/open_sans"/>
    <TextView
            android:text="@string/probedetails_comment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView26"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/textView29"
            app:layout_constraintEnd_toEndOf="@+id/textView29" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintTop_toBottomOf="@+id/probedetails_regulationview" android:layout_marginTop="12dp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/probedetails_datecreatedview"
            app:layout_constraintTop_toTopOf="@+id/probedetails_backbutton" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="16dp" android:textSize="12sp"
            app:layout_constraintBottom_toBottomOf="@+id/probedetails_backbutton"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/probedetails_resultview"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="64dp"
            android:textSize="48sp" android:fontFamily="@font/open_sans_bold"
            android:textColor="@color/successColorDark" app:layout_constraintHorizontal_bias="0.501"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/probedetails_resultlabelview"
            android:textSize="28sp"
            android:fontFamily="@font/open_sans_semibold" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toBottomOf="@+id/probedetails_resultview"
            android:layout_marginTop="16dp" android:textColor="@color/successColorDark"/>
    <TextView
            android:layout_width="301dp"
            android:layout_height="wrap_content" android:id="@+id/probedetails_resultdescview"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="12dp" app:layout_constraintTop_toBottomOf="@+id/probedetails_resultlabelview"
            android:gravity="clip_horizontal|center_horizontal" android:textColor="@color/darkGrey"
            android:fontFamily="@font/open_sans" app:layout_constraintHorizontal_bias="0.508"/>
    <TextView
            android:text="@string/probedetails_place"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView21"
            app:layout_constraintStart_toStartOf="parent"
            android:textStyle="bold" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.208" android:fontFamily="@font/open_sans"
            app:layout_constraintTop_toBottomOf="@+id/probedetails_resultdescview" android:layout_marginTop="16dp"/>
    <TextView
            android:text="@string/probedetails_location"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView22"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/textView26" app:layout_constraintEnd_toEndOf="@+id/textView26"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintTop_toBottomOf="@+id/probedetails_commentview" android:layout_marginTop="12dp"
            android:fontFamily="@font/open_sans"/>
    <TextView
            android:layout_width="180dp"
            android:layout_height="wrap_content" android:id="@+id/probedetails_locationview"
            app:layout_constraintTop_toTopOf="@+id/textView22"
            app:layout_constraintBottom_toBottomOf="@+id/textView22"
            android:autoSizeTextType="uniform"
            android:autoSizeMinTextSize="12sp"
            android:autoSizeMaxTextSize="100sp"
            android:autoSizeStepGranularity="2sp"
            app:layout_constraintStart_toEndOf="@+id/textView22" android:layout_marginStart="64dp"
            app:layout_constraintVertical_bias="0.0"/>
    <TextView
            android:layout_width="180dp"
            android:layout_height="wrap_content" android:id="@+id/probedetails_placeview"
            app:layout_constraintStart_toEndOf="@+id/textView21" android:layout_marginStart="64dp"
            app:layout_constraintTop_toTopOf="@+id/textView21"
            android:autoSizeTextType="uniform"
            android:autoSizeMinTextSize="12sp"
            android:autoSizeMaxTextSize="100sp"
            android:autoSizeStepGranularity="2sp"
            app:layout_constraintBottom_toBottomOf="@+id/textView21" app:layout_constraintVertical_bias="0.0"/>
    <TextView
            android:layout_width="180dp"
            android:layout_height="wrap_content" android:id="@+id/probedetails_commentview"
            app:layout_constraintStart_toEndOf="@+id/textView26" android:layout_marginStart="44dp"
            app:layout_constraintTop_toTopOf="@+id/textView26"
            android:autoSizeTextType="uniform"
            android:autoSizeMinTextSize="12sp"
            android:autoSizeMaxTextSize="100sp"
            android:autoSizeStepGranularity="2sp"
            app:layout_constraintBottom_toBottomOf="@+id/textView26" app:layout_constraintVertical_bias="0.0"/>
    <TextView
            android:layout_width="180dp"
            android:layout_height="wrap_content" android:id="@+id/probedetails_regulationview"
            app:layout_constraintTop_toTopOf="@+id/textView29"
            app:layout_constraintBottom_toBottomOf="@+id/textView29"
            android:autoSizeTextType="uniform"
            android:autoSizeMinTextSize="12sp"
            android:autoSizeMaxTextSize="100sp"
            android:autoSizeStepGranularity="2sp"
            app:layout_constraintStart_toEndOf="@+id/textView29" android:layout_marginStart="44dp"
            app:layout_constraintVertical_bias="0.0"/>
    <Button
            android:layout_width="24dp"
            android:layout_height="30dp" android:id="@+id/probedetails_backbutton"
            app:layout_constraintTop_toTopOf="parent"
            style="?android:attr/borderlessButtonStyle"
            android:layout_marginTop="16dp"
            android:background="@drawable/arrow_back" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp"/>
    <TextView
            android:text="@string/probedetails_rating"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView6"
            app:layout_constraintTop_toBottomOf="@+id/probedetails_locationview"
            android:layout_marginTop="12dp" app:layout_constraintStart_toStartOf="@+id/textView22"
            app:layout_constraintEnd_toEndOf="@+id/textView22" app:layout_constraintHorizontal_bias="0.0"
            android:fontFamily="@font/open_sans" android:textStyle="bold"/>
    <TextView
            android:layout_width="180dp"
            android:layout_height="wrap_content" android:id="@+id/probedetails_ratingview"
            app:layout_constraintStart_toEndOf="@+id/textView6" android:layout_marginStart="28dp"
            app:layout_constraintTop_toTopOf="@+id/textView6" app:layout_constraintBottom_toBottomOf="@+id/textView6"
            app:layout_constraintVertical_bias="1.0"/>

</androidx.constraintlayout.widget.ConstraintLayout>